		<Tomador>
			<IdentificacaoTomador>
				<CpfCnpj>
					{{#cnpj}}
	  				<Cnpj>{{cnpj}}</Cnpj>  				
					{{/cnpj}}

					{{#cpf}}
	  				<Cpf>{{cpf}}</Cpf>  				
					{{/cpf}}					
				</CpfCnpj>
			</IdentificacaoTomador>
			{{#razao_social}}
			<RazaoSocial>{{razao_social}}</RazaoSocial>
			{{/razao_social}}
			<Endereco>
			  {{#endereco}}
